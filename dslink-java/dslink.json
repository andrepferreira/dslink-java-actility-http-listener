{
  "name": "http-listener-dslink",
  "version": "0.0.1",
  "description": "Simple Java DSLinks http listener",
  "license": "Apache",
  "author": {
    "name": "YOUR NAME",
    "email": "YOUR EMAIL"
  },
  "main": "bin/http-listener-dslink",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/template-dslink-java"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/template-dslink-java/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "http-listener-dslink"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.template.Main"
    }
  }
}
